<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ProyectoWeb</display-name>
  <servlet>
    <servlet-name>bienvenidoServlet</servlet-name>
    <servlet-class>Servlets.LoginController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>altaProducto</servlet-name>
    <servlet-class>Servlets.AltaProductoController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bienvenidoServlet</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>altaProducto</servlet-name>
    <url-pattern>/altaProductoController</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>